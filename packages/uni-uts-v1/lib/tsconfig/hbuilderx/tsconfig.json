{
  "extends": "../tsconfig.vue.json",
  "compilerOptions": {
    "baseUrl": ".",
    "verbatimModuleSyntax": true,
    "lib": ["ESNext"],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "env.d.ts",
    "shim-uni.d.ts",
    "global.d.ts",
    "**/*.uts",
    "**/*.ts",
    "**/*.d.ts",
    "**/*.tsx",
    "**/*.vue",
    "**/*.uvue"
  ]
}
